{
  "name": "recurse-center",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "author": "benjamin",
  "scripts": {
    "dev": "tsup src/index.ts --config tsup.config.ts --watch",
    "build": "tsup src/index.ts --config tsup.config.ts",
    "start": "node --loader ts-node/esm examples/index.ts",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typedoc": "^0.26.4",
    "typedoc-plugin-markdown": "^4.2.1",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@badgateway/oauth2-client": "^2.3.0",
    "ky": "^1.4.0",
    "unfetch": "^5.0.0"
  }
}
